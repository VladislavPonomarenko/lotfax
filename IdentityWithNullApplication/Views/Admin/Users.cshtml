@model IEnumerable<IdentityWithNullApplication.Models.ApplicationUser>

@{ 
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <h4>Users</h4>
    <table class="table table-hover table-responsive table-striped table-bordered">
        <tr>
            <th>ID</th>
            <th>User Name</th>
            <th>Email</th>
            <th>EmailConfirmed</th>
            <th>EmailConfirmed</th>

        </tr>

        @foreach (IdentityWithNullApplication.Models.ApplicationUser user in Model)
        {
        <tr>
            <td>@user.Id</td>
            <td>@user.UserName</td>
            <td>@user.Email</td>

            <td>@user.EmailConfirmed</td>
          
            @if (@user.Roles.Equals("admin"))
    {
        <td>admin</td>
}
else
{
        <td>not admin</td>
}

        </tr>
        }
    </table>
</div>

